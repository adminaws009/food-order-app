{
  "family": "food-backend",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "awsadmin009/food-backend:latest",  // Replace with your Docker Hub username
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0  // Let ECS assign a dynamic port
        }
      ]
    }
  ]
}
