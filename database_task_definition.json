{
  "family": "food-database",
  "containerDefinitions": [
    {
      "name": "database",
      "image": "awsadmin009/food-database:latest",  // Replace with your Docker Hub username
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3306,
          "hostPort": 0  // Let ECS assign a dynamic port
        }
      ]
    }
  ]
}
